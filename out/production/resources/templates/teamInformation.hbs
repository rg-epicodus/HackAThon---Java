{{#partial "content"}}
    <p>Team {{ team.teamName }}</p>
    <ul id="teamList">
        <li>{{ team.teamDescription }}</li>
        <li>{{ team.teamMembers }}</li>
    </ul>
    <h5>Created At: {{ team.teamCreatedAt }}</h5>
    <p><a href="/teams/{{team.id}}/update">Edit this Team</a></p>
{{/partial}}

{{> layout.hbs}}